        SuperscriptBox["E", 
         RowBox[{
          RowBox[{"-", "I"}], " ", "Pi", " ", "m", " ", 
          RowBox[{"x", "/", "2"}]}]]}], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", 
         RowBox[{"-", "n"}], ",", "n"}], "}"}]}], "]"}]}], ";"}]}]}]], "Input",
