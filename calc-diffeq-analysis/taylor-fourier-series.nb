     RowBox[{
      FractionBox["1", "4"], 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "[", "x", "]"}], " ", 
         SuperscriptBox["E", 
          RowBox[{"I", " ", "Pi", " ", "n", " ", 
           RowBox[{"x", "/", "2"}]}]]}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"-", "2"}], ",", "2"}], "}"}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"fourier", "[", 
      RowBox[{"f_", ",", "n_", ",", "x_"}], "]"}], ":=", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"fouriercoeff", "[", 
