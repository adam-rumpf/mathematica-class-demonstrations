    RowBox[{"and", " ", "variable", " ", "x"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"fouriercoeff", "[", 
      RowBox[{"f_", ",", "0"}], "]"}], ":=", 
     RowBox[{
      FractionBox["1", "4"], 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{"f", "[", "x", "]"}], ",", 
        RowBox[{"{", 
