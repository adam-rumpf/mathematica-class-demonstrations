Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "explicit", " ", "definition", " ", "of", " ", "Taylor", " ", "series", 
     " ", "for", " ", "function", " ", "f"}], ",", " ", 
    RowBox[{"n", " ", "terms"}], ",", " ", 
    RowBox[{"center", " ", "c"}], ",", " ", 
    RowBox[{"and", " ", "variable", " ", "x"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"taylor", "[", 
    RowBox[{"f_", ",", "n_", ",", "c_", ",", "x_"}], "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{
           RowBox[{"f", "[", "x", "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "i"}], "}"}]}], "]"}], "/.", 
         RowBox[{"x", "\[Rule]", "c"}]}], ")"}], 
       RowBox[{"i", "!"}]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"x", "-", "c"}], ")"}], "i"]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.7766008761831923`*^9, 3.776600882799075*^9}, {
  3.7767147153423014`*^9, 3.7767147524331293`*^9}}],

Cell[BoxData[
